<?xml version="1.0" encoding="utf-8"?>
<full-backup-content>
    <!-- Include only non-sensitive app data first -->
    <include domain="file" path="logs/" />

    <!-- Then exclude sensitive data from backup -->
    <exclude domain="sharedpref" path="." />
    <exclude domain="database" path="." />
    <exclude domain="file" path="cache/" />
    <exclude domain="file" path="tmp/" />
</full-backup-content>
